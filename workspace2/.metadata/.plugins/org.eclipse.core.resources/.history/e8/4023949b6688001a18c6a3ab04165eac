package com.jdbc.main;

import java.sql.SQLException;
import java.util.ArrayList;

import com.jdbc.dao.TelInfoDAO;
import com.jdbc.vo.TelInfoVO;

public class Test {

	public static void main(String[] args) throws ClassNotFoundException, SQLException {
		TelInfoDAO dao = new TelInfoDAO();
		
		ArrayList<TelInfoVO> result = dao.getAllInfo();
		
		System.out.println("ID : " + result.get(0).getId());
		System.out.println("이름 : " + result.get(0).getName());
		System.out.println("전화번호 : " + result.get(0).getTel());
		System.out.println("등록 날짜 : " + result.get(0).getD());

	}

}
