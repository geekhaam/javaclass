package com.java.gui;

import java.awt.Frame;

public class HelloGUI2 extends Frame {
	public HelloGUI2() {
		// Default Constructor
	}
	public HelloGUI2(String title) {
		super.setTitle(title);
	}
	
	public static void main(String[] args) {
		HelloGUI2 h2 = new HelloGUI2("Hello GUI2");
		//객체 생성 + 초기치 => 생성자 필요
	}
}
